{
    "basePath": "/api",
    "endpoints": [
        {
            "method": "POST",
            "path": "/signup/patient",
            "description": "Endpoint to register a new patient.",
            "requestBody": {
                "name": "string",
                "birthdate": "Date",
                "contact": "string",
                "insuranceDetails": "string",
                "password": "string"
            },
            "successResponse": {
                "code": 201,
                "message": "Patient created successfully.",
                "data": {
                    "patientId": "int"
                }
            },
            "errorResponses": [
                {
                    "code": 400,
                    "message": "Bad Request - Missing or invalid input parameters."
                },
                {
                    "code": 409,
                    "message": "Conflict - Patient already exists."
                }
            ]
        },
        {
            "method": "POST",
            "path": "/login/staff",
            "description": "Endpoint for staff members to log in.",
            "requestBody": {
                "userId": "string",
                "password": "string"
            },
            "successResponse": {
                "code": 200,
                "message": "Staff login successful.",
                "data": {
                    "token": "string",
                    "userType": "staff"
                }
            },
            "errorResponses": [
                {
                    "code": 401,
                    "message": "Unauthorized - Incorrect staff credentials."
                },
                {
                    "code": 500,
                    "message": "Internal Server Error."
                }
            ]
        },
        {
            "method": "POST",
            "path": "/login/patient",
            "description": "Endpoint for patients to log in.",
            "requestBody": {
                "userId": "string",
                "password": "string"
            },
            "successResponse": {
                "code": 200,
                "message": "Patient login successful.",
                "data": {
                    "token": "string",
                    "userType": "patient"
                }
            },
            "errorResponses": [
                {
                    "code": 401,
                    "message": "Unauthorized - Incorrect patient credentials."
                },
                {
                    "code": 500,
                    "message": "Internal Server Error."
                }
            ]
        },
        {
            "method": "POST",
            "path": "/tests/addtest",
            "description": "Endpoint for patients to log in.",
            "requestBody": {
                "userId": "string",
                "password": "string"
            },
            "successResponse": {
                "code": 200,
                "message": "Patient login successful.",
                "data": {
                    "token": "string",
                    "userType": "patient"
                }
            },
            "errorResponses": [
                {
                    "code": 401,
                    "message": "Unauthorized - Incorrect patient credentials."
                },
                {
                    "code": 500,
                    "message": "Internal Server Error."
                }
            ]
        }
    ]
}